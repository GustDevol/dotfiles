alias v='vim'
alias nv='nvim'
alias rm='rm -i'
alias a='sudo apt'
alias py='python3'
alias nano='nano -$'
alias mycodes='cd /media/$USER/F/mycodes'
alias clr_cache='sudo sh -c "sync; echo 3 > /proc/sys/vm/drop_caches"'
